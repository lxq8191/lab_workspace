package com.test;

import java.util.Arrays;

public class Solution1 {
	public int[] twoSum(int[] nums, int target) {
		int[] result = new int[2];
		int head = 0,tail = nums.length-1;
		Arrays.sort(nums);
		for(;head <= tail&&tail > 0&&nums[head]+nums[tail]!=target;){
			
		}
		return result;
    }
}
