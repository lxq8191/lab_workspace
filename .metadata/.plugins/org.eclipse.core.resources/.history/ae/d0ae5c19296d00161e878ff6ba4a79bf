package com.test;

import java.util.Arrays;

public class Solution1 {
	public int[] twoSum(int[] nums, int target) {
		int[] result = new int[2];
		int temp = 0;
		int head = 0,tail = nums.length-1;
		Arrays.sort(nums);
		for(;head <= tail&&tail > 0;){
			temp = nums[head]+nums[tail];
			if(temp==target){
				break;
			}else if(temp>target){
				tail--;
			}else if(temp<target){
				head++;
			}
			
		}
		return result;
    }
}
