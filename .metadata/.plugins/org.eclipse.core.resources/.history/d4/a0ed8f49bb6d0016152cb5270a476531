package com.test;

public class Solution2 {
	class ListNode {
	    int val;
	    ListNode next;
	    ListNode(int x) { val = x; }
	}
	
	public ListNode addTwoNumbers(ListNode l1, ListNode l2) {
		while(l1!=null&&l2!=null){
			l1.val = l1.val + l2.val;
			l1 = l1.next;
			l2 = l2.next;
		}
		return l1;
    }
}
