package com.test;


class ListNode {
    int val;
    ListNode next;
    ListNode(int x) { val = x; }
}

public class Solution21 {
    public ListNode mergeTwoLists(ListNode l1, ListNode l2) {
		if(l1==null&&l2==null) return null;
		if(l1!=null&&l2==null) return l1;
		if(l1==null&&l2!=null) return l2;
		
    	return l2;
    }
}
